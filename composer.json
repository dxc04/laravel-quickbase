{
  "name": "dxc04/laravel-quickbase",
  "description": "A wrapper around rossity/php-quickbase for Laravel.",
  "license": "MIT",
  "authors": [
    {
      "name": "Ross Ball",
      "email": "w.ross.ball@gmail.com",
      "homepage": "https://github.com/rossity"
    }
  ],
  "homepage": "https://github.com/rossity/laravel-quickbase",
  "keywords": [
    "rossity",
    "laravel",
    "php-quickbase",
    "openapitools",
    "openapi-generator",
    "openapi",
    "php",
    "sdk",
    "rest",
    "api"
  ],
  "require": {
    "illuminate/support": "~7|~8|~9",
    "dxc04/php-quickbase": "*@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0",
    "orchestra/testbench": "~5|~6"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/dxc04/php-quickbase.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rossity\\LaravelQuickbase\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rossity\\LaravelQuickbase\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rossity\\LaravelQuickbase\\LaravelQuickbaseServiceProvider"
      ],
      "aliases": {
        "Quickbase": "Rossity\\LaravelQuickbase\\Facades\\Quickbase"
      }
    }
  },
  "minimum-stability": "dev"
}
